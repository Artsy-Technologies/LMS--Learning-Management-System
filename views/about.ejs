}

.team-item:hover {
    transform: translateY(-5px);
    box-shadow: 0 8px 20px rgba(0, 0, 0, 0.15);
}

.team-item .overflow-hidden {
    height: 200px; /* Increased image container height */
    overflow: hidden;
    flex-shrink: 0;
    position: relative;
}

.team-item .overflow-hidden img {
    width: 100%;
    height: 100%;
    object-fit: contain; /* Changed from cover to contain to show full image */
    object-position: center;
    background-color: #f8f9fa; /* Light background for better contrast */
    padding: 10px; /* Add some padding around the image */
    transition: transform 0.3s ease;
}

.team-item:hover .overflow-hidden img {
    transform: scale(1.05);
}

/* Alternative approach - if you want to fit the entire image without padding */
.team-item .overflow-hidden img.fit-image {
    object-fit: scale-down; /* This will scale down large images to fit completely */
    max-width: 100%;
    max-height: 100%;
    width: auto;
    height: auto;
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
}

.team-item .text-center {
    flex-grow: 1;
    display: flex;
    flex-direction: column;
